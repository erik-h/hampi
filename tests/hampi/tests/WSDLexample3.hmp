var v : 70;
cfg Address:= "<ns:Address>" Recipient House Street Town County PostCode Country "</ns:Address>";
cfg Recipient := Recipientt ;
cfg Recipientt := "<ns:Recipient>" string "</ns:Recipient>";
cfg House := Houset ;
cfg Houset := "<ns:House>" string "</ns:House>";
cfg Street := Streett ;
cfg Streett := "<ns:Street>" string "</ns:Street>";
cfg Town := Townt ;
cfg Townt := "<ns:Town>" string "</ns:Town>";
cfg County :=   | Countyt ;
cfg Countyt := "<ns:County>" string "</ns:County>";
cfg PostCode := PostCodet ;
cfg PostCodet := "<ns:PostCode>" string "</ns:PostCode>";
cfg Country :=   | Countryt ;
cfg Countryt := "<ns:Country>" null "</ns:Country>";
cfg null := "Restrict";
cfg digit := ['0'-'9'];
cfg char := ['!'-'~'];
cfg string := (char)*;
cfg double := digit "." (digit)+;
assert v in Address;
